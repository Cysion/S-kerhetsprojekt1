{
    "logs":{
        "%%dev_note":"all times in section are in days, sizes are in bytes",
        "archived_for":730,
        "max_raw_size":2048
    },
    "media":{
        "media_base_dir":"user",
        "max_size_mb":"100",
        "max_per_user":"250"
    },
    "research":{
        "%%dev_note":["timestrf is used in naming containing folders for where entries are stored"],
        "timestrf":"%Y%m%d%H%M%S",
        "exportdir":"research",
        "enable_collection":"True",
        "output_to_console":"True",
        "key_lifetime":600
    }
}
